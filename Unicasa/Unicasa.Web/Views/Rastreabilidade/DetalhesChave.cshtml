<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
    <h4 class="modal-title">Detalhes do Pedido</h4>
</div>
<div class="modal-body">
    <div class="row">
        <div class="col-md-12 bold">
            <div class="table-scrollable">
                <table class="table table-hover table-light">
                    <thead>
                        <tr>
                            <th> # </th>
                            <th> Detalhe </th>
                            <th> Cliente </th>
                            <th> Status </th>
                            <th> Ação </th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model.Tickets != null)
                        {
                            foreach (var itens in Model.Tickets)
                            {
                                <tr>
                                    <td> @itens.Detalhe </td>
                                    <td> @itens.Descricao </td>
                                    <td> @itens.Cliente </td>
                                    <td> @itens.TicketState.ToString() </td>
                                    <td>
                                        <a class="btn dark btn-xs btn-outline" data-toggle="modal" data-dismiss="modal" data-target="#subDetalhes" href="@Url.Action("Detalhes", "Rastreabilidade", new { id = itens.ImportacaoId })">Detalhes</a>
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<div class="modal-footer">
    <button type="button" class="btn dark btn-outline" data-dismiss="modal">Fechar</button>
</div>


